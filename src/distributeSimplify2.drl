rule "Double Negate"
     when
          e0 : SymbolicElem( )
          n0 : SymbolicNegate( elem == e0 )
          n1 : SymbolicNegate( elem == n0 )
          not( Reng( strt == n1 ) )
     then
          insert( new Reng( n1 , e0 ) );
end






