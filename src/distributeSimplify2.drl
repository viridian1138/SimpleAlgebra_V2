rule "Init Add Root"
      when
          elA : SymbolicAdd( )
      then
          // System.out.println( "Init Add Root " + elA );
          insert( new AddRoot( elA , elA ) );
end





rule "Add RootA"
      when
          el0 : SymbolicAdd()
          elA : AddRoot( elemB == el0 )
          elB : SymbolicAdd( elemA == el0 )
      then
          // System.out.println( "Add Root A " + elA.getElemA() + " " + elB );
          insert( new AddRoot( elA.getElemA() , elB ) );
end





